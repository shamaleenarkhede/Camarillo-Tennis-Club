@model Camarillo_Tennis_Club.Models.Players

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New Player", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BDate)
        </th>
        
    </tr>

@foreach (var item in Model.playersList) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BDate)
        </td>
      
    </tr>
}

</table>

<div>
    @Html.ActionLink("Back to Admin Home Page", "AdminHome","Login")
</div>